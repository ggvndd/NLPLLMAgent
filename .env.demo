# Demo Environment Variables (for testing without API keys)
# Copy this to .env for demo/testing mode

# Discord Bot Configuration (optional for CLI testing)
DISCORD_BOT_TOKEN=demo_token

# LLM Provider Configuration
# Set to "demo" to run without API keys
LLM_PROVIDER=demo

# Demo API Keys (not real - for demonstration only)
OPENAI_API_KEY=demo_openai_key
ANTHROPIC_API_KEY=demo_anthropic_key

# Logging Configuration
LOG_LEVEL=INFO
LOG_DIR=logs

# Application Environment
ENVIRONMENT=development